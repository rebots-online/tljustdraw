{
  "name": "@tljustdraw/shared-utils",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"__tests__/**/*.ts\"",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"__tests__/**/*.ts\""
  },
  "dependencies": {
    "tslib": "^2.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  }
}
